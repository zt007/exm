  alge = {
   {x^5 + 23 x^4 - 2 x^3 + 67 x^2 + x + 5, {-37, -4}},
   
   {435848050125 + 222766781175 x + 55277585837730 x^2 - 9805282551729 x^3 + 334572270902514 x^4 -
   52731895464720 x^5 + 1426118565888567 x^6 - 151760537383518 x^7 + 4210807159069560 x^8 + 
   350112658289094 x^9 + 4266536744740142 x^10 + 642068017899503 x^11 + 359343908863309 x^12 + 
   70221377134284 x^13 + 53634236327503 x^14 + 5371613888441 x^15 + 1679270409080 x^16 + 
   203267000250 x^17 - 4955990625 x^18 + 19765625 x^19 + 1953125 x^20, {-104, -9}},
     
   {5 x^4 + x^3 - x^2 + 15 x - 9, {-3, 0}},
   
   {528144561510886039041234081 - 2167685267402776691633601096 x^2 - 303975141065717313385780761096 x^4 + 
     6625874678751583937877473236699 x^6 +  20641022773328629742444921989962 x^8 - 
     131203217813360999547064715081515 x^10 -  650665758485924934924369475503477 x^12 + 
     546209074011727692013584655130773 x^14 + 14298932902913075466429698097208866 x^16 + 
     37737267975539954779760917635730708 x^18 +  472650385217569973736200169572187 x^20 - 
     1570825528456154827560663024522 x^22 +  4025418628843686965680568745 x^24 - 
     28098753509656176676796007 x^26 + 85824434036875995308196 x^28 - 
     74061756484369815816 x^30 + 58041336376873989 x^32 - 26140422654297 x^34 + 10460353203 x^36, 
     {-(101/1024), 101/512 + (101 I)/256}},
     
   {3 x^6 + 6 x^4 - 31 x^3 - 3 x^2 - x + 9, {0, 1}},
   
   {-11065116586329 - 45017055513612 x^2 + 211100682408057 x^4 - 
     274894510989735 x^6 + 126716834090907 x^8 - 8363309577147 x^10 - 
     7768785178078 x^12 - 157188127404 x^14 + 225902302553 x^16 + 
